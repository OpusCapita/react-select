{
  "name": "@opuscapita/react-select",
  "version": "2.0.1-SNAPSHOT",
  "description": "react-select with opuscapita styles",
  "scripts": {
    "link-mode": "webpack --watch --config ./webpack.link.config.js",
    "lint": "eslint src/client",
    "start": "showroom-scan src && node www/index",
    "gh-pages-generate": "showroom-scan src && sh scripts/gh-pages/generate.sh",
    "gh-pages-deploy": "npm run gh-pages-generate && sh scripts/gh-pages/deploy.sh .gh-pages-tmp",
    "npm-build": "rimraf lib && cross-env NODE_ENV=production webpack --config ./webpack.production.config.js --hide-modules",
    "npm-publish": "npm run npm-build && npm publish",
    "publish-release": "npm run npm-publish"
  },
  "repository": "OpusCapita/react-select",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "files": [
    "lib",
    "README.md"
  ],
  "readme": "README.md",
  "peerDependencies": {
    "react": "^15.6.2 || ^16.2.0",
    "react-dom": "^15.6.2 || ^16.2.0"
  },
  "devDependencies": {
    "@opuscapita/npm-scripts": "2.0.0",
    "@opuscapita/react-showroom-client": "1.3.0-beta.6",
    "@opuscapita/react-showroom-server": "1.3.0",
    "autoprefixer": "6.7.6",
    "babel-cli": "6.23.0",
    "babel-core": "6.23.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.3.2",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-0": "6.22.0",
    "babel-register": "6.23.0",
    "chai": "3.5.0",
    "chai-enzyme": "0.6.1",
    "compression": "1.6.2",
    "cross-env": "5.0.5",
    "css-loader": "0.26.2",
    "enzyme": "2.7.1",
    "eslint": "3.17.0",
    "eslint-config-opuscapita": "1.0.4",
    "eslint-plugin-react": "6.10.0",
    "express": "4.15.1",
    "file-loader": "0.10.1",
    "ignore-styles": "5.0.1",
    "istanbul": "0.4.5",
    "jsdom": "9.12.0",
    "json-loader": "0.5.4",
    "less": "2.7.2",
    "less-loader": "2.2.3",
    "nodemon": "1.11.0",
    "postcss-loader": "1.3.3",
    "postcss-modules": "0.6.4",
    "raw-loader": "0.5.1",
    "react": "15.6.2",
    "react-addons-test-utils": "15.4.2",
    "react-dom": "15.6.2",
    "rimraf": "2.6.1",
    "sinon": "2.1.0",
    "sinon-chai": "2.9.0",
    "source-map-loader": "0.1.6",
    "style-loader": "0.13.2",
    "url-loader": "0.5.8",
    "webpack": "2.2.1",
    "webpack-dev-middleware": "1.10.1",
    "write-file-webpack-plugin": "3.4.2"
  },
  "dependencies": {
    "react-select": "2.0.0"
  }
}
